<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-preview-i18n-resource dnanalytics.properties?>

<VBox id="VBox" alignment="TOP_CENTER" fillWidth="true" spacing="3.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dnanalytics.view.SettingsController">
  <children>
        <Label id="paramName" text="%label.genome" />
            <TextField id="fileTextField" fx:id="genome" editable="false" maxWidth="1.7976931348623157E308" onMouseClicked="#selectGenome" prefWidth="-1.0" promptText="%file.fasta" HBox.hgrow="ALWAYS" />
        <Label id="paramName" text="%tempDir" />
        <TextField id="fileTextField" fx:id="tempDir" editable="false" onMouseClicked="#selectTempDir" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label id="paramName" text="%language" />
        <ChoiceBox fx:id="languagesBox">
          <tooltip>
            <Tooltip text="Choose language" />
          </tooltip>
        </ChoiceBox>
        <Label text="%language.restart" />
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
