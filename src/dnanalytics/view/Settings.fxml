<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-preview-i18n-resource dnanalytics.properties?>

<VBox id="VBox" alignment="TOP_CENTER" fillWidth="true" spacing="3.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="dnanalytics.view.SettingsController">
  <children>
    <HBox alignment="CENTER" fillHeight="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="NEVER">
      <children>
        <Separator prefWidth="-1.0" HBox.hgrow="SOMETIMES" />
        <Label text="%label.genome" />
        <Separator prefWidth="-1.0" HBox.hgrow="SOMETIMES" />
      </children>
    </HBox>
    <VBox id="VBox" alignment="CENTER" spacing="5.0">
      <children>
        <HBox prefHeight="-1.0" prefWidth="-1.0" spacing="3.0">
          <children>
            <TextField fx:id="genome" editable="false" maxWidth="1.7976931348623157E308" prefWidth="-1.0" promptText="%file.fasta" HBox.hgrow="ALWAYS" />
            <Button defaultButton="true" mnemonicParsing="false" onAction="#selectGenome" text="%button.browse" />
          </children>
        </HBox>
      </children>
    </VBox>
    <HBox alignment="CENTER" fillHeight="true" prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Separator prefWidth="-1.0" HBox.hgrow="SOMETIMES" />
        <Label text="%tempDir" />
        <Separator prefWidth="-1.0" HBox.hgrow="SOMETIMES" />
      </children>
    </HBox>
    <HBox prefHeight="-1.0" prefWidth="-1.0" spacing="3.0">
      <children>
        <TextField fx:id="tempDir" editable="false" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Button defaultButton="true" mnemonicParsing="false" onAction="#selectTempDir" text="%button.browse" />
      </children>
    </HBox>
    <HBox alignment="CENTER" fillHeight="true" prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Separator prefWidth="-1.0" HBox.hgrow="SOMETIMES" />
        <Label text="%language" />
        <Separator prefWidth="-1.0" HBox.hgrow="SOMETIMES" />
      </children>
    </HBox>
    <VBox id="VBox" alignment="CENTER" spacing="5.0">
      <children>
        <ChoiceBox fx:id="languagesBox">
          <tooltip>
            <Tooltip text="Choose language" />
          </tooltip>
        </ChoiceBox>
        <Label text="%language.restart" />
      </children>
    </VBox>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
