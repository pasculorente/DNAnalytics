<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-preview-i18n-resource dnanalytics.properties?>
<?scenebuilder-stylesheet DNAnalytics.css?>

<BorderPane pickOnBounds="false" prefHeight="776.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml" fx:controller="dnanalytics.view.DNAMain">
  <center>
    <SplitPane id="SplitPane" dividerPositions="0.39974937343358397" orientation="HORIZONTAL">
      <items>
        <TabPane id="tabPane" disable="false" focusTraversable="false" rotate="0.0" rotateGraphic="false" side="TOP" tabClosingPolicy="UNAVAILABLE" tabMaxHeight="1.7976931348623157E308" tabMinHeight="0.0" tabMinWidth="0.0" visible="true">
          <tabs>
            <Tab id="toolsTab" text="">
              <content>
                <VBox id="VBox" alignment="TOP_CENTER" opacity="1.0" snapToPixel="true" spacing="5.0">
                  <children>
                    <TilePane fx:id="buttonsPane" alignment="TOP_CENTER" hgap="2.0" prefColumns="5" prefHeight="-1.0" prefWidth="-1.0" vgap="2.0" />
                    <TitledPane fx:id="currentTool" animated="false" text="untitled">
                      <content>
                        <AnchorPane id="Content" prefHeight="180.0" />
                      </content>
                    </TitledPane>
                    <Button defaultButton="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#start" text="%button.start" />
                  </children>
                  <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x4" />
                  </padding>
                </VBox>
              </content>
              <graphic>
                <Label id="toolsTab" text="%tab.tools" />
              </graphic>
            </Tab>
            <Tab closable="false" disable="false" text="">
              <content>
                <VBox id="VBox" alignment="TOP_CENTER" fillWidth="true" spacing="3.0">
                  <children>
                    <TitledPane alignment="TOP_CENTER" animated="true" collapsible="false" contentDisplay="TEXT_ONLY" text="%label.genome">
                      <content>
                        <VBox id="VBox" alignment="CENTER" spacing="5.0">
                          <children>
                            <HBox prefHeight="-1.0" prefWidth="-1.0" spacing="3.0">
                              <children>
                                <TextField fx:id="genome" editable="false" maxWidth="1.7976931348623157E308" prefWidth="-1.0" promptText="%label.genome" HBox.hgrow="ALWAYS" />
                                <Button defaultButton="true" mnemonicParsing="false" onAction="#selectGenome" text="%button.browse" />
                              </children>
                            </HBox>
                            <Label fx:id="alreadyIndexed" alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" text="" wrapText="true" />
                          </children>
                        </VBox>
                      </content>
                    </TitledPane>
                    <TitledPane alignment="TOP_CENTER" animated="true" collapsible="false" contentDisplay="TEXT_ONLY" text="%tempDir">
                      <content>
                        <HBox prefHeight="-1.0" prefWidth="-1.0" spacing="3.0">
                          <children>
                            <TextField fx:id="tempDir" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                            <Button defaultButton="true" mnemonicParsing="false" onAction="#selectTempDir" text="%button.browse" />
                          </children>
                        </HBox>
                      </content>
                    </TitledPane>
                    <TitledPane alignment="TOP_CENTER" animated="true" collapsible="false" contentDisplay="TEXT_ONLY" text="%language">
                      <content>
                        <VBox id="VBox" alignment="CENTER" spacing="5.0">
                          <children>
                            <ChoiceBox fx:id="languagesBox">
                              <tooltip>
                                <Tooltip text="Choose language" />
                              </tooltip>
                            </ChoiceBox>
                            <Label text="%language.restart" />
                          </children>
                        </VBox>
                      </content>
                    </TitledPane>
                  </children>
                  <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x5" />
                  </padding>
                </VBox>
              </content>
              <graphic>
                <Label id="settingsTab" text="%tab.settings" />
              </graphic>
            </Tab>
          </tabs>
        </TabPane>
        <TabPane fx:id="consoleTabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="ALL_TABS">
          <tabs>
            <Tab closable="false" text="%tab.console">
              <content>
                <TextArea fx:id="console" editable="false" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" text="" wrapText="true">
                  <stylesheets>
                    <URL value="@DNAnalytics.css" />
                  </stylesheets>
                </TextArea>
              </content>
            </Tab>
          </tabs>
        </TabPane>
      </items>
    </SplitPane>
  </center>
</BorderPane>
